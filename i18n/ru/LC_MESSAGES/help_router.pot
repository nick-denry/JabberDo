msgid ".. - output existing lists"
msgstr ""

#: help_router.py:33
msgid ".list_name - add or display list, i.e. `.tasks`. Also makes list \"active\"."
msgstr ""

#: help_router.py:34
msgid ". - display active list"
msgstr ""

#: help_router.py:35
msgid ".-- - clear tasks of the active list"
msgstr ""

#: help_router.py:36
msgid ".! - display completed tasks of the active list"
msgstr ""

#: help_router.py:37
msgid ".!- - clear completed tasks of the active list"
msgstr ""

#: help_router.py:38
msgid ".-list_name - delete list and all of its tasks"
msgstr ""

#: help_router.py:40
msgid "Any message - adds task to active list"
msgstr ""

#: help_router.py:41
msgid ":any_multiline_message - add multiple tasks from multiline message"
msgstr ""

#: help_router.py:42
msgid "!task_number [task_number] - move task(s) task_number to completed"
msgstr ""

#: help_router.py:43
msgid "-task_number - delete task task_number from list, i.e. -1 deletes first task."
msgstr ""

#: help_router.py:44
msgid ">task_number title - edit task task_number from current list, set it new title"
msgstr ""

#: help_router.py:45
msgid "^task_number list_name` - move task `task_umber` from current list to list_name list"
msgstr ""

#: help_router.py:47
msgid "*task_number time date - set reminder for task task_number to time/date"
msgstr ""

#: help_router.py:48
msgid ".* - list scheduled tasks"
msgstr ""

